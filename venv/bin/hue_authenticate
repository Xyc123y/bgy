#!/Users/xyc/PycharmProjects/bgy/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'python-hue-client==0.1.0','console_scripts','hue_authenticate'
__requires__ = 'python-hue-client==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-hue-client==0.1.0', 'console_scripts', 'hue_authenticate')()
    )
